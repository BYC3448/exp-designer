{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/shared/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/experiment/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/template/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/video/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/main.js",
      "dest": "/main.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 